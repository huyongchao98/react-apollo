    "predeploy": "npm run build",
    "deploy": "npm publish --tag beta"
  },
  "peerDependencies": {
    "apollo-client": "^2.6.3",
    "graphql": "^14.3.1",
    "react": "^16.8.0"
  },
  "dependencies": {
    "@apollo/react-common": "^2.5.9",
    "@apollo/react-components": "^2.5.11",
    "@apollo/react-hoc": "^2.5.11",
    "@apollo/react-hooks": "^2.5.11"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "rollup": "^1.15.5",
    "typescript": "^3.5.2"
  },
